{% set navItems = collections.all | eleventyNavigation %}
<nav class="mt-12 px-6">
    <ul class="flex flex-col space-y-4 md:flex-row md:justify-end md:space-x-6 md:space-y-0">
    {% for item in navItems %}
        <li><a href="{{ item.url }}"{% if page.url === item.url %} aria-current="page"{% endif %}>{{ item.title }}</a></li>
    {% endfor %}
    </ul>
</nav>
